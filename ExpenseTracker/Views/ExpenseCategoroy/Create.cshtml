@model ExpenseTracker.Models.ExpenseCategoroies

@{
    ViewData["Title"] = "Create";
}
@Html.Raw(TempData["msg"])

<h1>Create</h1>

<h4>ExpenseCategoroies</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddExpenseCategoroy">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
           
            <div class="form-group">
                <input type="hidden" asp-for="ExpenseCategoryID" />
                <label asp-for="CategoryName" class="control-label"></label>
                <input asp-for="CategoryName" class="form-control" />
                <span asp-validation-for="CategoryName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a asp-action="ExpenseCategoroyList" class="btn btn-outline-success">Back</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
